{
    "name": "multilanaguagehelloworld",
    "displayName": "MultiLanaguageHelloWorld",
    "description": "Multi Language Hello World VS Code extension permits insertion of \"Hello Wor",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/hello_js.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/hello_ts.json"
            },
            {
                "language": "c",
                "path": "./snippets/hello_c.json"
            },
            {
                "language": "cpp",
                "path": "./snippets/hello_cpp.json"
            }
        ]
    }
}
